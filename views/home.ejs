
<section id="quizzes-section">
    
    <div class="quiz-main-wrap">
        
        <% quizzes.forEach(quiz => {    %> 

            <div class="quiz" id="<%= quiz._id %> ">

                <h4> <%= quiz.title %> </h4>
                <div class="p-div">
                    <p> <%= quiz.description %>  </p>
                </div>
                <div class="toughness">
                    <p> <%= quiz.toughness %> </p>
                </div>
                
                <a href="/quizzes/<%= quiz._id %>"><button id="quiz-btn-home">Start Quiz</button></a>
                <!-- <a href="/instructor/results/<%= quiz._id %>">View Results</a> -->
            </div>
            
        <% }); %> 

    </div>

</section>
